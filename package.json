{
  "name": "amosai-cli",
  "version": "0.1.0",
  "description": "Generate JS projects templates we use",
  "main": "dist/index.js",
  "repository": "https://github.com/Amosai-Project/amosai-cli.git",
  "author": "Santiago Carrasco <jsanticc@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.js",
    "build": "babel src -d dist -s",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "update": "ncu -u && npm i",
    "clean": "rimraf dist"
  },
  "bin": {
    "amosai-cli": "./dist/index.js"
  },
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "amosai",
    "create-project"
  ],
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "execa": "^4.0.0",
    "inquirer": "^7.1.0",
    "listr": "^0.14.3",
    "ncp": "^2.0.0",
    "ora": "^4.0.4",
    "pkg-install": "^1.0.0",
    "rimraf": "^3.0.2",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3",
    "npm-check-updates": "^4.1.2"
  }
}
